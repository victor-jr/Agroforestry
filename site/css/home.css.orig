@-ms-viewport{
    width: extend-to-zoom;
    zoom: 1.0;
}

h1 {
    font-size: 1.6em;
    margin: 0.83em 0;
}
h2 {
    font-size: 1.3em;
    margin: 1em 0;
}
h3 {
    font-size: 1.17em;
    margin: 1.33em 0;
}
h4 {
    font-size: 0.9em;
}

body {
    font-family: 'Open Sans Hawaii', 'Open Sans', 'Segoe UI', 'Lucida Grande', sans-serif;
    font-size: 1.125em;
    margin: 0;
    background: white;
    color: black;

    text-rendering: optimizeLegibility; /* optional for older browsers */
    -webkit-font-variant-ligatures: common-ligatures; /* for iOS and Safari 6 */
    font-variant-ligatures: common-ligatures; /* for up-to-date browsers, including IE10 and Opera 21 */
}
img {
    width: auto;
    height: auto;
}

h1, h2, h3, h4 {
    font-family: 'Alegreya', serif;
    color: #592A1E;
    line-height: 105%;
}

.okina {
    font-family: 'Open Sans Hawaii';
}

#content_skip {
    height: 1px;
    width: 1px;
    margin: -1px;
    overflow: hidden;
    position: absolute;
}

.header {
    position: relative;
    width: 100%;
    z-index: 1000;
}

.uhh-header {
    height: 33px;
    background: #fbfaf7;
    border-top: 4px solid #E01F22;
    border-bottom: 1px solid #233B3D;
    width: 100%;
}

.dept-header {
    background: #006633;
    height: 150px;
    border-bottom: 1px solid #233B3D;
    width: 100%;
}

.header img {
    display: block;
}

.header-inner {
    position: relative;
    width: 100%;
    height: 100%;
    max-width: 1240px;
    margin: 0 auto;
}

.header .uhh-seal {
    display: block;
    position: absolute;
    left: 25px;
    top: -3px;
}
.header .uhh-seal img {
    height: 140px;
    border: 0;
}

.header .uhh-name {
    position: absolute;
    left: 190px;
    top: 6px;
}
.header .uhh-name img {
    height: 24px;
    border: 0;
}

.header .banner {
    position: absolute;
    /*left: 190px;
    top: 14px;*/
    left: 25px;
    top: 0px;
    color: white;
    font-family: 'Alegreya', serif;
    font-size: 40px;
    line-height: 105%;
}
.header .banner img {
    height: 150px;
    border: 0;
}

.header a {
    text-decoration: none;
    color: inherit;
}

.menu {
    position: absolute;
    bottom: 5px;
    left: 80px;
		width: 1020px;
		z-index: 9999;
}
.menu > .inner {
}
.menu ul {
    list-style: none;
    margin: 0;
    padding: 0;
}
.menu li {
    display: inline-block;
    margin-right: 30px;
}
.menu a {
    display: inline-block;
    color: #E5D45E;
    -webkit-transition: color 0.2s linear;
    -moz-transition: color 0.2s linear;
    -o-transition: color 0.2s linear;
    transition: color 0.2s linear;
}
.menu a:hover, .menu li.active a {
    color: white;
}

.flag-menu {
    position: absolute;
    bottom: 2px;
		left: 1020px;
		text-align: center;
		font-size: 12px;
		z-index: 9999;
}

.flag-menu > .flag-inner {
}

.flag-menu a {
    display: inline-block;
    color: #E5D45E;
    -webkit-transition: color 0.2s linear;
    -moz-transition: color 0.2s linear;
    -o-transition: color 0.2s linear;
    transition: color 0.2s linear;
}
.flag-menu a:hover {
    color: white;
}

.header a:active {
    position: relative;
    top: 1px;
}
.go-to-menu {
    display: none;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    width: 66px;
}
.go-to-menu:hover, body.show-menu .go-to-menu {
    background: rgba(255,255,255,0.20);
}
.go-to-menu a {
    line-height: 32px;
    font-size: 10px;
    color: #ddd;
    text-transform: uppercase;
    font-weight: bold;
    text-align: center;
    position: absolute;
    height: 100%;
    width: 100%;
}
.go-to-menu a:before, .go-to-menu a:after {
    position: absolute;
    top: 26px;
    left: 22px;
    content: '';
    display: block;
    width: 22px;
    height: 5px;
    border-top: 4px solid #ddd;
    border-bottom: 4px solid #ddd;
}
.go-to-menu a:after {
    top: 39px;
    border-top: 0
}
.go-to-menu a:active {
    position: absolute;
}

#footer {
    clear: both;
    background-color: #ACBEDA;
    padding-bottom: 5px;
    font-size: 0.8em;
    margin-top: 20px;
}
#innerfooter {
    max-width: 900px;
    margin: 0 auto 0;
    padding: 1em;
}
#footer * {
    color: #3D3D3D;
}
ul.footerlinks {
    margin: 0;
    margin-top: .5em;
    display: block;
    padding: 0;
    text-align: center;
}
ul.footerlinks li {
    display: inline-block;
    padding-right: 10px;
    font-size: 1.125em;
}
ul.footerlinks li:last-child {
    padding-right: 0;
}
#footer a {
    text-decoration: none;
    color: #103A3D;
}
#footer a:hover {
    text-decoration: underline;
}

#content {
    max-width: 1000px;
    margin: 0 auto;
    padding: 10px;
    min-height: 500px;
}

